    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Start server (ganache)",
            "program": "${workspaceFolder}/start_server.js"
        }, {
            "type": "node",
            "request": "launch",
            "name": "Start server (rinkeby)",
            "program": "${workspaceFolder}/start_server.js",
            "env": {
                "STREAMR_API_KEY": "NIwHuJtMQ9WRXeU5P54f6A6kcv29A4SNe4FDb06SEPyg",
                "ETHEREUM_NETWORK": "rinkeby",
                "ETHEREUM_PRIVATE_KEY": "6E340F41A1C6E03E6E0A4E9805D1CEA342F6A299E7C931D6F3DA6DD34CB6E17D",
                "TOKEN_ADDRESS": "0x0c89795f75eEA923Cd1faEea50777c1B87787A06"
            }
        }, {
            "type": "node",
            "request": "launch",
            "name": "Run current file",
            "program": "${file}"
        }, {
            "type": "node",
            "request": "launch",
            "name": "Mocha test current file",
            "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/mocha",
            "env": {
                "DEBUG": "*,-mocha:*"
            },
            "runtimeArgs": [
                "${file}",
                "--exit"
            ],
            "internalConsoleOptions": "openOnSessionStart"
        }, {
            "type": "node",
            "request": "launch",
            "name": "Etherlime test current file",
            "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/etherlime",
            "runtimeArgs": [
                "test",
                "--solcVersion=0.4.24",
                "${file}"
            ],
            "internalConsoleOptions": "openOnSessionStart"
        }
    ]
}